@{
    ViewBag.Title = "Manage Data";
}
@section PageTitleH1
{
    <h1><i class="glyphicon glyphicon-bookmark"></i> @ViewBag.Title</h1>
}
@section Breadcumb
{
    <ol class="breadcrumb">
        <li><a href="@Url.Action("Index","Home")">Home</a></li>
        <li class="active">Manage Data</li>        
    </ol>
}
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Water Quality Sampling Data</h3>
            </div>
            <div class="panel-body">
                <ul>
                    <li><a href="@Url.Action("Query","Analysis")"><i class="glyphicon glyphicon-stats"></i> Query, Graph and Visualize Data</a></li>
                    <li><a href="@Url.Action("Upload")"><i class="glyphicon glyphicon-upload"></i> Upload a Lab EDD file</a></li>
                    <li><a href="@Url.Action("Samples_List")">Manage Water Quality Samples</a></li>
                    <li><a href="@Url.Action("Analytes_List")">Manage Analytes</a></li>                    
                    
                    
                </ul>
            </div>            
        </div>
    </div>
</div> <!-- // row -->
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-info">
            <div class="panel-heading">
                <h3 class="panel-title">Manage Associated Data</h3>
            </div>
            <div class="panel-body">
                <ul>
                    <li><a href="@Url.Action("SamplingSites_List")"><i class="glyphicon glyphicon-map-marker"></i> Manage Sampling Sites</a></li>
                    <li><a href="@Url.Action("Projects_List")"><i class="glyphicon glyphicon-th"></i> Manage Projects</a></li> 
                    <li><a href="@Url.Action("OrgsPeople")"><i class="glyphicon glyphicon-user"></i> Manage Labs, Organizations and People</a></li>                   
                </ul>
            </div>
        </div>
    </div>
</div> <!-- // row -->

