@{
    ViewBag.Title = "Import ESDAT Data";
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Import ESDAT Data</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-cloud-upload fa-fw"></i> Import ESDAT data
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <ol class="round">
                        <li class="one">
                            <h5>@Html.ActionLink("Import ESDAT data from local file system", "ImportFromLocal", "ESDAT")</h5>
                            <br />
                        </li>
                        <li class="two">
                            <h5>@Html.ActionLink("Import ESDAT data from HTTP file system", "ImportFromHttp", "ESDAT")</h5>
                            <br />
                        </li>
                        <li class="three">
                            <h5>@Html.ActionLink("Import ESDAT data from FTP file system", "ImportFromFtp", "ESDAT")</h5>
                            <br />
                        </li>
                        <li class="four">
                            <h5>@Html.ActionLink("View ODM2 data in raw format", "ViewImportedData", "ESDAT")</h5>
                            <br />
                        </li>
                    </ol>
                </div>
                <!-- /.panel-body -->
            </div>
            <!-- /.panel -->

        </div>
    </div>
</div>

